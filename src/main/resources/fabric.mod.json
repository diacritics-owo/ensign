{
  "schemaVersion": 1,
  "id": "ensign",
  "version": "${version}",
  "name": "Ensign",
  "description": "Improvements and additions for banners",
  "authors": ["diacritics-owo"],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/ensign/icon.png",
  "environment": "*",
  "accessWidener": "ensign.accesswidener",
  "entrypoints": {
    "main": ["diacritics.owo.Ensign"],
    "client": ["diacritics.owo.EnsignClient"]
  },
  "mixins": [
    "ensign.mixins.json",
    {
      "config": "ensign.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.21",
    "java": ">=21",
    "fabric-api": "*"
  }
}
